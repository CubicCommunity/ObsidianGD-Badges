{
	"geode": "3.8.1",
	"gd": {
		"win": "2.206",
		"android": "2.206",
		"mac": "2.206",
		"ios": "2.206"
	},
	"id": "cheeseworks.obsidiangd_badges",
	"name": "ObsidianGD Badges",
	"version": "v1.1.3",
	"developers": [
		"Cheeseworks"
	],
	"description": "Adds a badge to ObsidianGD members' profiles.",
	"tags": [
		"online",
		"offline",
		"enhancement"
	],
	"links": {
		"source": "https://github.com/CubicCommunity/ObsidianGD-Badges/",
		"community": "https://www.discord.com/invite/nhtyT6hDwa",
		"homepage": "https://www.obsidianmg.cc/gd-team/"
	},
	"issues": {
		"info": "You may report issues with the mod in the ObsidianGD-Badges repository",
		"url": "https://github.com/CubicCommunity/ObsidianGD-Badges/issues/"
	},
	"resources": {
		"sprites": [
			"resources/*.png"
		]
	},
	"dependencies": [
		{
			"id": "geode.node-ids",
			"version": ">=1.15.0",
			"importance": "required"
		},
		{
			"id": "jouca.badgesapi",
			"version": ">=1.1.0",
			"importance": "required"
		}
	],
	"api": {
		"include": [
			"src/util/*.hpp"
		]
	},
	"settings": {
		"display": {
			"name": "Display",
			"description": "Customize badge display.",
			"type": "title"
		},
		"show-profile": {
			"name": "Display On Profile",
			"description": "Display member badges on the profile page.",
			"type": "bool",
			"default": true
		},
		"show-comments": {
			"name": "Display On Comments",
			"description": "Display member badges on comment cells.",
			"type": "bool",
			"default": true
		},
		"ui": {
			"name": "Interface",
			"description": "UI-related functions.",
			"type": "title"
		},
		"err-notifs": {
			"name": "Web Error Notifications",
			"description": "Display a notification when the badge web request fails.",
			"type": "bool",
			"default": true
		},
		"dev": {
			"name": "Developers",
			"description": "Utilities for development purposes.",
			"type": "title"
		},
		"console": {
			"name": "Log In Console",
			"description": "Log all mod actions in the console.",
			"type": "bool",
			"default": false
		}
	}
}